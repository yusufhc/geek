NameVirtualHost *:80

#################################################################
#
# All other HTTP Domains
#
#################################################################
<VirtualHost *:80>

  ServerName flush
  DocumentRoot /var/www/html

  ErrorDocument 404 http://www.bp.com
  ErrorDocument 403 http://www.bp.com
  ErrorDocument 500 http://www.bp.com
  ErrorDocument 503 http://www.bp.com

  RewriteEngine On

  Include "/etc/httpd/conf.d/urlredirect_http_default.conf"
  RewriteRule    ^/$  http://www.bp.com  [NC,L,R]

</VirtualHost>

#################################################################
#
# bp.com
#
#################################################################
<VirtualHost *:80>

  ServerName www.bp.com
  ServerAlias origin-prod-www-navitas.bp.com
  ServerAlias bp.com

  DocumentRoot /var/www/html

  ErrorDocument 404 http://www.bp.com
  ErrorDocument 403 http://www.bp.com
  ErrorDocument 500 http://www.bp.com
  ErrorDocument 503 http://www.bp.com

  RewriteEngine On

  Include "/etc/httpd/conf.d/urlredirect_bp.conf"

  ErrorLog "/var/log/httpd/bp_error.log"
  CustomLog /var/log/httpd/bp_access.log "%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-agent}i\""

</VirtualHost>

